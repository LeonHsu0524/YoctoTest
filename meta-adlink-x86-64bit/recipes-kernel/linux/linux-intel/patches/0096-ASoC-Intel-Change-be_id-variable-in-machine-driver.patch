+		.id = 1,
 		.cpu_dai_name = "SSP2 Pin",
 		.codec_name = "snd-soc-dummy",
 		.codec_dai_name = "snd-soc-dummy-dai",
@@ -514,7 +514,7 @@ static int broxton_gpmrb_tdf8532_startup(struct snd_pcm_substream *substream)
 	{
 		/* SSP2-B - Dirana */
 		.name = "SSP2-B-Codec",
-		.be_id = 1,
+		.id = 1,
 		.cpu_dai_name = "SSP2-B Pin",
